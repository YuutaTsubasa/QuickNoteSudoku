{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "快速筆記數獨",
  "version": "1.0.0",
  "identifier": "com.yuutatsubasa.quicknotesudoku",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "快速筆記數獨",
        "width": 600,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "android": {
      "versionCode": 1105,
      "minSdkVersion": 35
    }
  }
}
